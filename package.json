    "debug": "./node_modules/.bin/babel src --out-dir debug --watch",
    "deploy": "webpack --release && npm run build-server",
    "dev": "node dev-server.js -c ./webpack.hotdev.config",
    "eslint": "eslint src",
    "start-dev": "./node_modules/.bin/babel-node src/server/index.js --verbose",
    "start": "node build/server/index.js --release"
  },
